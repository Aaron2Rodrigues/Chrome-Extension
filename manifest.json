{
    "manifest_version": 3,
    "name": "Font Identifier",
    "version": "1.0",
    "description": "Identify fonts on a webpage.",
    "permissions": ["activeTab"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"], 
            "js": ["content.js"]
        }
    ]
}